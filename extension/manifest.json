{
  "manifest_version": 3,
  "name": "Project Inocula Shield",
  "version": "1.0",
  "description": "Analyzes page credibility to fight misinformation.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}